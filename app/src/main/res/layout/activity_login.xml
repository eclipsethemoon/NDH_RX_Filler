<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.selu.neilhoang.ndh_rx_filler.LoginActivity">


    <EditText
        android:id="@+id/userWNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/enter_w"
        android:hint="Enter W #"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/loginBtn"
        android:background="@color/colorPrimary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/userWNum"
        android:layout_alignStart="@+id/userWNum"
        android:layout_below="@+id/userWNum"
        android:text="@string/login" />


</RelativeLayout>
